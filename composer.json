{
    "name": "radiergummi/flysystem-github-storage",
    "description": "A GitHub Storage filesystem for Flysystem.",
    "keywords": [
        "radiergummi",
        "flysystem-github",
        "flysystem",
        "github"
    ],
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "psr/log": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Moritz Friedrich",
            "email": "m@9dev.de",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Radiergummi\\FlysystemGitHub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radiergummi\\FlysystemGitHub\\Tests\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
